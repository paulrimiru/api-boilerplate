{
  "name": "school-api",
  "version": "1.0.0",
  "description": "small api for a school management system",
  "main": "src/server.js",
  "scripts": {
    "postinstall": "tsc",
    "start": "pm2 start pm2.yaml",
    "stop": "pm2 stop pm2.yaml",
    "start:dev": "ts-node --files -T --project ./tsconfig.json -r tsconfig-paths/register src/server.ts",
    "start:dev:watch": "nodemon",
    "test": "jest --coverage --coverageReporters=text",
    "test:dev": "yarn test --watch",
    "coveralls": "NODE_ENV=test jest --coverage --coverageReporters=text-lcov | coveralls",
    "lint": "tslint --project . --fix",
    "build": "tsc",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/PaulKariukiRimiru/school-api.git"
  },
  "author": "Paul Rimiru",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PaulKariukiRimiru/school-api/issues"
  },
  "homepage": "https://github.com/PaulKariukiRimiru/school-api#readme",
  "devDependencies": {
    "@types/compression": "^0.0.36",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.11",
    "@types/mongoose": "^5.3.23",
    "@types/node": "^11.11.3",
    "@types/redis": "^2.8.11",
    "@types/supertest": "^2.0.7",
    "@types/swagger-ui-express": "^3.0.0",
    "coveralls": "^3.0.3",
    "fp-ts": "^1.15.0",
    "jest": "^24.5.0",
    "mongoose": "^5.4.19",
    "nodemon": "^1.18.10",
    "supertest": "^4.0.2",
    "swagger-ui-express": "^4.0.2",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.3",
    "tslint": "^5.14.0",
    "tslint-microsoft-contrib": "^6.1.0",
    "typescript": "^3.3.4000"
  },
  "dependencies": {
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "5.0.0-alpha.7",
    "nyc": "^13.3.0",
    "pg": "^7.9.0",
    "pg-hstore": "^2.3.2",
    "pm2": "^4.4.0",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "6.19.1",
    "sequelize-typescript": "^0.6.8",
    "ts-node-dev": "^1.0.0-pre.32",
    "tsconfig-paths": "^3.8.0",
    "snyk": "^1.189.0"
  },
  "snyk": true
}
